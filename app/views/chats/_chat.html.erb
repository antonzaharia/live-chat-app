<%= turbo_frame_tag dom_id(chat) do %>
  <%= link_to chat_path(chat), data: { turbo: false }, class: 'card bg-primary text-primary-content mb-2' do %>
    <div class="card-body p-3">
      <h2 class="card-title"><%= chat.name %></h2>
      <p class="text-xs" >
        <% chat.users.each do |user| %>
          <%= user.email %>
        <% end %>
      </p>
    </div>
  <% end %>
<% end %>
